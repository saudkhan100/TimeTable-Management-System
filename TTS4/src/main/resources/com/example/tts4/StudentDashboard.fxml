<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="CENTER" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/17.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.tts4.StudentDashboardController">
   <Label text="Student Dashboard">
      <font>
         <Font size="18.0" />
      </font>
   </Label>
   <Label text="Student Name:" GridPane.rowIndex="1" />
   <Label fx:id="nameLabel" text="" GridPane.rowIndex="1" GridPane.columnIndex="1" />
   <Label text="Department:" GridPane.rowIndex="2" />
   <Label fx:id="departmentLabel" text="" GridPane.rowIndex="2" GridPane.columnIndex="1" />
   <Label text="Section:" GridPane.rowIndex="3" />
   <Label fx:id="sectionLabel" text="" GridPane.rowIndex="3" GridPane.columnIndex="1" />
   <Label text="Timetable:" GridPane.rowIndex="4" GridPane.columnIndex="0" />
   <GridPane fx:id="timetableGrid" GridPane.rowIndex="4" GridPane.columnIndex="1" hgap="10" vgap="10">
      <columnConstraints>
         <ColumnConstraints hgrow="SOMETIMES" minWidth="100.0" />
         <ColumnConstraints hgrow="SOMETIMES" minWidth="100.0" />
         <ColumnConstraints hgrow="SOMETIMES" minWidth="100.0" />
         <ColumnConstraints hgrow="SOMETIMES" minWidth="100.0" />
         <ColumnConstraints hgrow="SOMETIMES" minWidth="100.0" />
      </columnConstraints>
      <rowConstraints>
         <RowConstraints minHeight="30.0" vgrow="SOMETIMES" />
      </rowConstraints>
   </GridPane>
</GridPane>
